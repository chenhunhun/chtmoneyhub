<?xml version="1.0" encoding="utf-8"?>
<main>
  <category name="banks">

<site id="sdb" name="深圳发展银行"  version="2.0.0.0">
			<domains>
				<domain name="*.sdb.com.cn/*" />
			</domains>	
			<requirements>
				<system type="All" cpu="32">
					<require type="activex" name="sdbPower" reg="reg-32">
						<file name="PowerEnterSDB.ocx" />
					</require>
					<require type="driver" name="PECKP" service="Protectorsdb">
						<file name="PECKP.sys" />
					</require>
					<require type="file" name="PECKP.sys" path="%SystemRoot%\system32\drivers" replace="false">
					</require>
				</system>
				<system type="All" cpu="64">
					<require type="activex" name="PowerEnterSDB" reg="reg-64">
						<file name="PowerEnterSDB.ocx" />
					</require>
					<require type="driver" name="PECKP_x64" service="Protectorsdb64">
						<file name="PECKP_x64.SYS" />
					</require>
					<require type="file" name="PECKP_x64.SYS" path="%SystemRoot%\system32\drivers" replace="false">
					</require>
					</system>
			</requirements>
			<page url="https://ebank.sdb.com.cn/perbank/logon_pro.jsp">
			        <require name="loalii" />
			</page>
            <page url="https://ebank.sdb.com.cn/corporbank/logon_basic.jsp">
			        <require name="loalii" />
			</page>				
</site>
  </category>  
</main>